{
  "name": "hola-ui",
  "version": "1.0.0",
  "description": "Vue 3 compatible Hola UI",
  "main": "index.js",
  "repository": "https://github.com/rhp-island/hola-ui.git",
  "author": "albert trantoan.fox.97@gmail.com",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "yarn run lerna bootstrap",
    "build": "lerna run build",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "scaffold": "hygen generator module --name",
    "playground:routes": "ts-node ./scripts/parse-routes.ts",
    "playground:build": "yarn install && yarn build && yarn playground:routes && NODE_ENV=production vite build playground",
    "dev": "yarn playground:routes && NODE_ENV=development vite serve playground --config ./vite.config.ts"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/css": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@types/jest": "^29.0.0",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/recursive-readdir": "^2.2.1",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/compiler-sfc": "^3.2.38",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vuedx/typecheck": "^0.7.5",
    "@vuedx/typescript-plugin-vue": "^0.7.5",
    "concurrently": "^7.3.0",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "css-get-unit": "^1.0.1",
    "csstype": "^3.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "fs-extra": "^10.1.0",
    "hygen": "^6.2.8",
    "lerna": "^3.22.1",
    "lodash.mergewith": "^4.6.2",
    "object-assign": "^4.1.1",
    "prettier": "^2.7.1",
    "recursive-readdir": "^2.2.2",
    "tinycolor2": "^1.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "dependencies": {
    "@hola-ui/styled-system": "^1.0.14-alpha.0",
    "change-case": "^4.1.2"
  }
}
